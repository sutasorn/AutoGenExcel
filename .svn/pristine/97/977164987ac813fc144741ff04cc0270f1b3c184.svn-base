html {
    -webkit-tap-highlight-color: transparent;
    -webkit-touch-callout: none;
    text-rendering: optimizeLegibility
}

body,
body * {
    vertical-align: baseline;
    border: 0;
    outline: 0;
    padding: 0;
    margin: 0;
    font: 100%/1.5 Arial, sans-serif
}

#container_ad {
    -ms-transform-origin: 0% 0%;
    -webkit-transform-origin: 0% 0%;
    -moz-transform-origin: 0% 0%;
    -o-transform-origin: 0% 0%;
    position: absolute;
    width: 158px;
    height: 598px;
    overflow: hidden;
    border: 1px solid #DDD;
    top: 0px;
    left: 0px;
    margin: auto
}

#content_ad {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0px;
    left: 0px;
    z-index: 100;
    background: #fff
}

#background_exit_ad {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0px;
    left: 0px;
    cursor: pointer;
    opacity: 0;
    z-index: 100;
}

#hd1,
#hd2,
#hd3,
#tag,
#logo,
#product {
    position: absolute;
    top: 0;
    left: 0;
    width: 160px;
    height: 600px;
    background-size: 160px 600px;
    opacity: 0

